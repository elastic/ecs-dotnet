<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <IsPackable>false</IsPackable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Argu" Version="6.0.0" />
    <PackageReference Include="Bullseye" Version="3.3.0" />
    <PackageReference Include="Proc" Version="0.6.2" />
    <PackageReference Include="Fake.Tools.Git" Version="5.15.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="Paths.fs" />
    <Compile Include="CommandLine.fs" />
    <Compile Include="Targets.fs" />
    <Compile Include="Program.fs" />
    <Content Include="..\..\.github\workflows\opentelemetry.yml">
      <Link>opentelemetry.yml</Link>
    </Content>
    <Content Include="..\..\.github\workflows\pre-commit.yml">
      <Link>pre-commit.yml</Link>
    </Content>
    <Content Include="..\..\.github\workflows\test-reporter.yml">
      <Link>test-reporter.yml</Link>
    </Content>
    <Content Include="..\..\.github\workflows\test.yml">
      <Link>test.yml</Link>
    </Content>
  </ItemGroup>

</Project>
