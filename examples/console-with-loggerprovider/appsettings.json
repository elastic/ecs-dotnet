{
  "Logging": {
    "Elasticsearch": {
      "ConnectionPoolType": "SingleNode",
      "NodeUris": [ "http://localhost:9200" ],
      "Tags": [ "Development" ],
      "IndexOffset": "-12:00",
      "IsEnabled": true,
      "IncludeScopes": true,
      "IncludeHost": true,
      "IncludeProcess": true,
      "IncludeUser": true
    },
    "LogLevel" : {
      "Default": "Trace",
      "Microsoft": "Warning"
    }
  }
}