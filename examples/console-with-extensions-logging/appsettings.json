{
  "Logging": {
    "Elasticsearch": {
      "Tags": [ "Development", "Example" ],
      "IndexOffset": "00:00",
      "IsEnabled": true,
      "IncludeScopes": true,
      "IncludeHost": true,
      "IncludeProcess": true,
      "IncludeUser": true,
      "ShipTo": {
        "ConnectionPoolType": "SingleNode",
        "NodeUris": [ "http://localhost:9200" ]  
      }
    },
    "LogLevel" : {
      "Default": "Trace",
      "Microsoft": "Warning"
    }
  }
}